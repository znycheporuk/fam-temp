{
  "name": "fam-website",
  "author": "Nycheporuk Zakhar",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev -c \"bun run server.ts\"",
    "start": "NODE_ENV=production bun run server.ts",
    "typecheck": "tsc",
    "setup": "bun install && prisma generate",
    "db:migrate": "prisma generate && prisma migrate",
    "db:push": "prisma generate && prisma db push"
  },
  "dependencies": {
    "@prisma/client": "4.16.2",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@remix-run/server-runtime": "^1.18.1",
    "i18next": "^23.2.6",
    "i18next-fs-backend": "^2.1.5",
    "i18next-http-backend": "^2.2.1",
    "isbot": "^3.6.8",
    "nodemailer": "^6.9.3",
    "rc-notification": "^4.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "remix-i18next": "^5.2.0",
    "ua-parser-js": "^1.0.35",
    "xml-formatter": "^3.4.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.18.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/ua-parser-js": "^0.7.36",
    "autoprefixer": "^10.4.14",
    "bun-types": "0.6.13",
    "postcss-preset-env": "^8.5.1",
    "prisma": "^4.16.2",
    "typescript": "^4.6.2"
  },
  "engines": {
    "bun": ">=0.6.13"
  }
}
